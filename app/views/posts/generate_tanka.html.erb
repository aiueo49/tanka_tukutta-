<%= form_with model: @post, url: posts_path, method: :post, data: { turbo: false } do |f| %>
  <div class="flex flex-col">
    <% session[:tankas].each_with_index do |tanka, index| %>
      <div class="mb-4">
        <%= f.radio_button :content, tanka, id: "tanka#{index}", name: "post[content]" %>
        <%= f.label "tanka#{index}", tanka %>
      </div>
    <% end %>
    <%= f.submit "決定", class: "mt-4 px-4 py-2 bg-pink-500 text-white rounded" %>
  </div>
<% end %>